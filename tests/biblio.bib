@Article{Arrington:1992xu,
  author       = {Arrington, J. and Beise, E.J. and Filippone, B.W. and O'Neill, T.G. and Dodge, W.R. and others},
  title        = {{A Variable energy Moller polarimeter at the MIT Bates Linear Accelerator Center}},
  year         = {1992},
  volume       = {A311},
  pages        = {39-48},
  doi          = {10.1016/0168-9002(92)90849-Y},
  abstract     = {A M{\o}ller polarimeter has been developed for operation on Beam Line B at the MIT-Bates Linear Accelerator Center. The beam polarization is inferred from measurement of the asymmetry in polarized electron-electron elastic scattering from a Fe-Co foil of known polarization, using a large-bore quadrupole magnet to momentum-select the scattered electrons. This article reports measurements at $250~\MeV$ and $574~\MeV$, with a relative precision of $12\%$ when statistical and systematic errors are added in quadrature.},
