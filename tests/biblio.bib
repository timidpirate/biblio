@Article{Bartlett:1977gu,
  Title                    = {Is an electron's Charge Independent of Its Velocity?},
  Author                   = {Bartlett, D. F. and Ward, B. F. L.},
  Journal                  = {Phys. Rev. D},
  Year                     = {1977},
  Pages                    = {3453-3458},
  Volume                   = {16},

  Abstract                 = {We hypothesize that a particle's charge varies weakly with its velocity. Specifically, suppose that the charge of a slow electron (or proton) varies as q=e(1+kv2c2), where |k|<<1. This hypothesis can be tested for conduction electrons by rotating a current-carrying solenoid inside a Faraday ice pail. Alternatively, a block of metal may be cooled inside an ice pail. Using either of these two techniques we find |k|<10-5. For free electrons, we use electron beam measurements of others to determine that |k|<0.2. Finally a limit of |k|<2*10-15 is inferred for bound electrons from existing data on the neutrality of various atoms.},
  Doi                      = {10.1103/PhysRevD.16.3453},
  File                     = {:Articles/Bartlett1977gu.pdf:PDF},
  Slaccitation             = {%%CITATION = PHRVA,D16,3453;%%}
}

