@Article{Band:1997ee,
  author       = {Band, H.R. and Mitchell, G. and Prepost, R. and Wright, T.},
  title        = {{A Moller polarimeter for high-energy electron beams}},
  year         = {1997},
  volume       = {A400},
  pages        = {24-33},
  doi          = {10.1016/S0168-9002(97)00984-4},
  abstract     = {A single arm M{\o}ller polarimeter used to measure the longitudinal beam polarization of the $48~\GeV$ electron beam for SLAC fixed target experiment E-154 is described. The polarimeter utilizes an array of silicon strip detectors and a dipole magnetic spectrometer to detect M{\o}ller scattered electrons from magnetized Fe-Co alloy target foils. The details of the foil polarization measurements and analysis technique are discussed. The high statistical precision of the M{\o}ller data and the stability of the SLAC polarized source made precise studies of the systematic errors in the analysis possible. An overall systematic error of $2.7\%$ is assigned to the beam polarization determination.},
  file         = {:Articles/Band1997ee.pdf:PDF},
