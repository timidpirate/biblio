@Article{Khandramai:2011zd,
  Title                    = {{Four-loop QCD analysis of the Bjorken sum rule vs data}},
  Author                   = {Khandramai, V.L. and Pasechnik, R.S. and Shirkov, D.V. and Solovtsova, O.P. and Teryaev, O.V.},
