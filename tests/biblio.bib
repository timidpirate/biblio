@Article{Aleksejevs:2012xua,
  Title                    = {{Double-box contributions to Moeller scattering in the standard model}},
  Author                   = {Aleksejevs, A.G. and Barkanova, S.G. and Bystritskiy, Y.M. and Ilyichev, A.N. and Kuraev, E.A. and others},
  Journal                  = {Eur.Phys.J.},
  Year                     = {2012},
  Pages                    = {2249},
  Volume                   = {C72},

  Abstract                 = {The paper investigates contributions of $\gamma$, $Z$, $W$ and ghost intermediate states of box-type to the parity-violating M{\o}ller scattering asymmetry up to two-loop level. Using the Yennie-Frautschi-Suura factorization form for amplitudes, we demonstrate that QED corrections, with an exception of vacuum-polarization effects, cancel at the asymmetry level. We obtain chiral amplitudes at Born, one-loop and partially at two-loop level: ladder boxes and decorated boxes. Our calculations are relevant for the ultra-precise $11~\GeV$ M{\O}LLER experiment planned at Jefferson Laboratory. The numerical comparison of the two-loop contributions with the experimental accuracy of M{\O}LLER is provided.},
  Doi                      = {10.1140/epjc/s10052-012-2249-x},
  Slaccitation             = {%%CITATION = EPHJA,C72,2249;%%}
}

