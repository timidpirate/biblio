  Author                   = {Qattan, I. A. and Arrington, J. and Segel, R. E. and Zheng, X. and Aniol, K. and Baker, O. K. and Beams, R. and Brash, E. J. and Calarco, J. and Camsonne, A. and Chen, J.-P. and Christy, M. E. and Dutta, D. and Ent, R. and Frullani, S. and Gaskell, D. and Gayou, O. and Gilman, R. and Glashausser, C. and Hafidi, K. and Hansen, J.-O. and Higinbotham, D. W. and Hinton, W. and Holt, R. J. and Huber, G. M. and Ibrahim, H. and Jisonna, L. and Jones, M. K. and Keppel, C. E. and Kinney, E. and Kumbartzki, G. J. and Lung, A. and Margaziotis, D. J. and McCormick, K. and Meekins, D. and Michaels, R. and Monaghan, P. and Moussiegt, P. and Pentchev, L. and Perdrisat, C. and Punjabi, V. and Ransome, R. and Reinhold, J. and Reitz, B. and Saha, A. and Sarty, A. and Schulte, E. C. and Slifer, K. and Solvignon, P. and Sulkosky, V. and Wijesooriya, K. and Zeidman, B.},
  Journal                  = {Phys. Rev. Lett.},
  Year                     = {2005},
  Pages                    = {142301},
  Volume                   = {94},

  Abstract                 = {We report the results of a new Rosenbluth measurement of the proton form factors at Q^2 values of 2.64, 3.20 and 4.10 GeV^2. Cross sections were determined by detecting the recoiling proton in contrast to previous measurements in which the scattered electron was detected. At each Q^2, relative cross sections were determined to better than 1%. The measurement focussed on the extraction of G_E/G_M which was determined to 4-8% and found to approximate form factor scaling, i.e. \mu_p G_E \approx G_M. These results are consistent with and much more precise than previous Rosenbluth extractions. However, they are inconsistent with recent polarization transfer measurements of comparable precision, implying a systematic difference between the two techniques.},
  Archiveprefix            = {arXiv},
