@Article{Bartlett:1977gv,
  Title                    = {Spinning Magnets and Jehle's Model of the electron},
  Author                   = {Bartlett, D. F. and Monroy, J. and Reeves, J.},
  Journal                  = {Phys. Rev.},
  Year                     = {1977},
  Pages                    = {3459-3463},
  Volume                   = {D16},

  Abstract                 = {We have improved a classical test of unipolar induction. In this experiment a cylindrical sample of magnetized steel is spun about its axis inside a hollow, insulated steel sphere. According to the moving-line theory of unipolar induction the rotating magnetic field generates an electric field E-->=-(omega-->*r-->)*B--> in the region exterior to the magnet. This field will have the effect of elevating the potential of the enclosing sphere. We have searched in vain for such an effect and conclude that if the magnetic field lines are moved at all by the rotation of a magnet about its axis they are dragged with a coefficient less than 1.4*10-4. This limit is approximately 100 times more stringent than the 1912 measurement of Kennard. We discuss the application of this work to Jehle's recent model of leptonic charge.},
  Doi                      = {10.1103/PhysRevD.16.3459},
  File                     = {:Articles/Bartlett1977gv.pdf:PDF},
  Slaccitation             = {%%CITATION = PHRVA,D16,3459;%%}
}

