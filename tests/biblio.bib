  File                     = {:Articles/Schumacher2006cy.pdf:PDF;:Articles/Schumacher2006cy(1).pdf:PDF},
  Slaccitation             = {%%CITATION = HEP-PH/0609040;%%}
}

@Article{Schuren:1991sc,
