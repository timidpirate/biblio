@Article{Beenakker:1990maa,
  Title                    = {{QCD corrections to heavy quark production in hadron hadron collisions}},
  Author                   = {Beenakker, W. and van Neerven, W. L. and Meng, R. and Schuler, G. A. and Smith, J.},
