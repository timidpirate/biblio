@Article{Jadach:1993wk,
  Title                    = {{Soft pairs real and virtual infrared functions in QED}},
  Author                   = {Jadach, S. and Skrzypek, M. and Ward, B. F. L.},
  Journal                  = {Phys. Rev.},
  Year                     = {1994},
