@Article{Bardeen:1969aw,
  Title                    = {{Invariant amplitudes for photon processes}},
  Author                   = {Bardeen, William A. and Tung, W. K.},
  Journal                  = {Phys. Rev.},
  Year                     = {1968},
  Note                     = {[Erratum-ibid.D4:3229-3229,1971]},
  Pages                    = {1423-1433},
  Volume                   = {173},

  Abstract                 = {A method for constructing independent invariant amplitudes for photon processes which satisfy gauge invariance and are free from both kinematic singularities and zeros (constraints) is proposed. The method is based on the use of a simple projection operator which automatically ensures gauge invariance at every stage. The invariant amplitudes for pion photoproduction, pion Compton scattering, and nucleon Compton scattering are found. Previously known results in the first two cases are reproduced and their interpretation is clarified. For nucleon Compton scattering the result is new. Comparisons are made between these invariant amplitudes, the Hearn and Leader amplitudes, and the regularized helicity amplitudes. The Mandelstam representation may be applied to these kinematic-singularity-free and zero-free amplitudes without any ad hoc substractions. All known low-energy theorems on photon processes can be derived most directly from these invariant amplitudes.},
  Comment                  = {amplitudes without kinematic singularities},
  Doi                      = {10.1103/PhysRev.173.1423},
  Slaccitation             = {%%CITATION = PHRVA,173,1423;%%}
}

@Article{Barducci:1991rh,
  Title                    = {{Current quark mass and chiral symmetry breaking in QCD at finite temperature}},
  Author                   = {Barducci, A. and Casalbuoni, R. and De Curtis, S. and Gatto, Raoul and Pettini, Giulio},
  Journal                  = {Phys. Rev.},
  Year                     = {1992},
  Pages                    = {2203-2211},
  Volume                   = {D46},

  Abstract                 = {he effects of current quark masses on chiral-symmetry breaking in QCD at finite temperature are examined using a mean-field approximation. The current quark mass plays a role analogous to that of an external magnetic field in the ferromagnetic transition, as it explicitly violates the chiral symmetry whose restoration characterizes the phase transition. For small enough masses, the order parameter, related to the quark condensate, maintains the features of the massless case up to the critical temperature, and then it approaches a constant value depending on the current mass term itself. The consequences on the behavior of the pion mass and decay constant are also studied. Our calculations are done with a variational composite operator approach. We also discuss the critical exponents (in temperature, mass, and for susceptibility) that follow in general from assuming the possible validity of a Landau mean-field theory.},
  Doi                      = {10.1103/PhysRevD.46.2203},
  File                     = {:Articles/Barducci1991rh.pdf:PDF},
  Slaccitation             = {%%CITATION = PHRVA,D46,2203;%%}
}

@Article{Barducci:2000gc,
  Title                    = {{Chiral meson masses at finite temperature and density}},
  Author                   = {Barducci, A. and Casalbuoni, R. and Pettini, Giulio and Gatto, Raoul},
  Journal                  = {Phys. Rev.},
  Year                     = {2001},
  Pages                    = {074002},
  Volume                   = {D63},

  Abstract                 = {The ratio of the sigma mass to the pion mass at finite temperatures and densities provides for a quantitative signal of chiral symmetry breaking. We calculate this ratio by using an extension to finite chemical potential of the field theoretic composite operator formalism as applied to QCD. The calculation is limited to regions of the phase diagram where only quark-antiquark condensates dominate (no quark-quark condensates) and it confirms the expected behaviours. In particular the sigma becomes an essentially stable particle in a narrow region bordering the transition line from broken to restored chirality. This pattern is qualitatively the same both for the region where the transition is of second order as for the region where it is of first order (apart from the discontinuities expected in the latter case).},
  Archiveprefix            = {arXiv},
  Doi                      = {10.1103/PhysRevD.63.074002},
  Eprint                   = {hep-ph/0009285},
  File                     = {:Articles/Barducci2000gc.pdf:PDF},
  Slaccitation             = {%%CITATION = HEP-PH/0009285;%%}
}

