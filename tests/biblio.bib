  Author                   = {Abouzaid, E. and Arenton, M. and Barker, A. R. and Bellantoni, L. and Bellavance, A. and Blucher, E. and Bock, G. J. and Cheu, E. and Coleman, R. and Corcoran, M. D. and Cox, B. and Erwin, A. R. and Escobar, C. O. and Glazov, A. and Golossanov, A. and Gomes, R. A. and Gouffon, P. and Hanagaki, K. and Hsiung, Y. B. and Huang, H. and Jensen, D. A. and Kessler, R. and Kotera, K. and Ledovskoy, A. and McBride, P. L. and Monnier, E. and Nelson, K. S. and Nguyen, H. and Niclasen, R. and Phillips, D. G. and Ping, H. and Qi, X. R. and Ramberg, E. J. and Ray, R. E. and Ronquest, M. and Santos, E. and Shields, J. and Slater, W. and Smith, D. and Solomey, N. and Swallow, E. C. and Toale, P. A. and Tschirhart, R. and Velissaris, C. and Wah, Y. W. and Wang, J. and White, H. B. and Whitmore, J. and Wilking, M. J. and Winstein, B. and Winston, R. and Worcester, E. T. and Worcester, M. and Yamanaka, T. and Zimmerman, E. D. and Zukanovich, R. F.},
  Journal                  = {Phys. Rev.},
  Year                     = {2007},

  Month                    = {Jan},
  Number                   = {1},
  Pages                    = {012004},
  Volume                   = {D75},

  Abstract                 = {The branching ratio of the rare decay $\pi^0 \to e^+ e^-$ has been measured precisely, using the complete data set from the KTeV E799-II experiment at Fermilab. We observe 794 candidate $\pi^0 \to e^+ e^-$ events using $K^0_L \to 3\pi^0$ as a source of tagged $\pi^0$'s. The expected background is $52.7 \pm 11.2$ events, predominantly from high $e^+ e^-$ mass $\pi^0 \to e^+ e^- \gamma$ decays. We have measured $B[(\pi^0 \to e^+ e^-), (m_{e^+e^-}/m_{\pi^0})^2 > 0.95] = 6.44 \pm 0.25(\text{stat}) \pm 0.22(\text{syst}) \times 10^{-8}$, which is above the unitary bound from $\pi^0 \to \gamma \gamma$ and within the range of theoretical expectations from the standard model.},
  Archiveprefix            = {arXiv},
  Collaboration            = {KTeV},
  Doi                      = {10.1103/PhysRevD.75.012004},
  Eprint                   = {hep-ex/0610072},
  File                     = {:Articles/Abouzaid2006kk.pdf:PDF;:Articles/Abouzaid2006kk(1).pdf:PDF},
  Slaccitation             = {%%CITATION = HEP-EX/0610072;%%}
}

