@Article{Dorokhov:2009jd,
  Title                    = {How the recent BABAR data for $P \to \gamma\gamma^*$ affect the Standard Model predictions for the rare decays $P \to l^+l^-$},
  Author                   = {Dorokhov, A. E.},
