@Article{Arbuzov:2011zz,
  Title                    = {Processes $e^+ e^- \to \pi_0(\pi_0')$ gamma in extended NJL model},
  Author                   = {A.B. Arbuzov and E.A. Kuraev and M.K. Volkov},
  Journal                  = {Eur. Phys. J.},
  Year                     = {2011},
  Pages                    = {103},
  Volume                   = {A47},

  Abstract                 = {The processes of electron-positron annihilation into I (0) l and into I '(1300) l are considered within an extended NJL model. The intermediate vector mesons I (0), I I '(1450), and I (1420) are taken into account. The latter two mesons are treated as the first radial excited states. They are incorporated into the NJL model by means of a polynomial form factor. Numerical predictions for the cross-sections of these processes are received for the center-of-mass energies below 2 GeV. Our results for the I (0) l production are in agreement with experimental data obtained in the energy region 600-1020 MeV.},
  Owner                    = {Sai},
  Timestamp                = {2013.03.23}
}

